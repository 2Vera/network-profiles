#!/bin/sh

sshkeys="
ssh-rsa 
ssh-rsa  nickar@ingorian
ssh-rsa  panayotis@panayotis-ThinkPad-T450s
"
 
authkeys="/etc/dropbear/authorized_keys"
umask 077 ; mkdir -p $(dirname $authkeys) ; touch $authkeys
IFS="
"    
for key in $sshkeys ; do
  sed "\|^$key$|d" -i $authkeys
  echo   "$key"    >> $authkeys
done                           
